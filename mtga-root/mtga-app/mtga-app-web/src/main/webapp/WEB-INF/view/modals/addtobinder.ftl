<script src="<@spring.url '/javascript/browse/searchbinders.js' />" ></script>

<!-- Modal for adding cards to your binder-->
<div id="addToBinderModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="addToBinderLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 id="addToBinderLabel">Add to binder</h3>
  </div>
  <div class="modal-body">
    <p>One fine body…</p>
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
    <button class="btn btn-primary">Save changes</button>
  </div>
</div>